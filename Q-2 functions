# Define a function that returns Factorial of a number.
def func():
    a = int(input("enter a number"))
    factorial = 1
    if (a<0):
        print("factorial does not exist for negative no.")
    elif(a==0):
        print("for 0 factorial is 1")
    elif(a==1):
        print("for 1 factorial is 1")
    else:
        for i in range (1, a+1):
            factorial= factorial*i
    print(factorial)
func()
